<% if user_signed_in? %>
  <p>Welcome <%= current_user.email %></p>
  <div class="btn btn-default">
    <%= link_to 'Click to logout', destroy_user_session_path, :method => :delete %>
  </div>
<% else %>
  <p>You are not signed in.</p>
  <div class="btn btn-default">
    <%= link_to 'Login', new_user_session_path %>
  </div>
<% end %>